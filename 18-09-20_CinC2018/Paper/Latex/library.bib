isbn = {1875-6557 (Electronic)$\backslash$r1573-403X (Linking)},
issn = {15244571},
journal = {Circulation Research},
keywords = {arrhythmias,autonomic nervous system,cardiac,death,physiopathology,sudden cardiac},
month = {jun},
number = {12},
