journal = {Circulation},
keywords = {Folder - CinCpaper,Folder - SeniorProject - ResearchPaper,Magnetic resonance imaging,Myocardial infarction,Prognosis,magnetic resonance imaging,myocardial infarction,prognosis},
language = {en},
mendeley-tags = {Folder - CinCpaper,Folder - SeniorProject - ResearchPaper,magnetic resonance imaging,myocardial infarction,prognosis},
month = {jul},
number = {1},
pages = {32--39},
pmid = {16801462},
title = {{Characterization of the peri-infarct zone by contrast-enhanced cardiac magnetic resonance imaging is a powerful predictor of post-myocardial infarction mortality}},
url = {http://circ.ahajournals.org/content/114/1/32 http://circ.ahajournals.org/content/114/1/32.full http://circ.ahajournals.org/content/114/1/32.full.pdf http://www.ncbi.nlm.nih.gov/pubmed/16801462},
