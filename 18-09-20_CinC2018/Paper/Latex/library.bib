keywords = {ST segment depression,diagnosis,exercise electrocardiography,heart,heart rate,human,ischemic heart disease,short survey},
number = {5},
pages = {480--492},
title = {{Evaluation of the exercise electrocardiogram by the ST segment/heart rate slope}},
url = {http://www.embase.com/search/results?subaction=viewrecord{\&}from=export{\&}id=L17138315{\%}5Cnhttp://sfx.library.uu.nl/utrecht?sid=EMBASE{\&}issn=00287091{\&}id=doi:{\&}atitle=Evaluation+of+the+exercise+electrocardiogram+by+the+ST+segment/heart+rate+slope{\&}stitle=BULL.+NEW},
