doi = {10.1038/nature03817},
file = {:Users/Brian/GoogleDrive/Papers/Zhao, Samal, Srivastava - 2005 - Serum response factor regulates a muscle-specific microRNA that targets Hand2 during cardiogenesis.pdf:pdf},
