title = {{Heart rate adjustment of ST segment depression for improved detection of coronary artery disease}},
url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS{\&}PAGE=reference{\&}D=emed2{\&}NEWS=N{\&}AN=1989055496},
