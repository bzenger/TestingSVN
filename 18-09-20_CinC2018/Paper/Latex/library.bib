journal = {Journal of the American College of Cardiology},
keywords = {Aged,Atrial Fibrillation,Atrial Fibrillation: surgery,Catheter Ablation,Catheter Ablation: methods,Female,Follow-Up Studies,Heart Atria,Heart Atria: pathology,Humans,Imaging, Three-Dimensional,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Male,Middle Aged,Pulmonary Veins,Treatment Outcome},
