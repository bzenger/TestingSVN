issn = {15345807},
journal = {Developmental Cell},
number = {3},
pages = {450--459},
pmid = {20833366},
title = {{Hopx and Hdac2 interact to modulate Gata4 acetylation and embryonic cardiac myocyte proliferation}},
volume = {19},
year = {2010}
}
