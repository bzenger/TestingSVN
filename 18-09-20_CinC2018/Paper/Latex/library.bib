title = {{Subject-specific, multiscale simulation of electrophysiology: a software pipeline for image-based models and application examples.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=19414456{\&}retmode=ref{\&}cmd=prlinks{\%}5Cnpapers3://publication/doi/10.1098/rsta.2008.0314},
