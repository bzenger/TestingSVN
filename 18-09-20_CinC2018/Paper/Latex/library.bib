journal = {The American journal of physiology},
keywords = {Animals,Arrhythmias, Cardiac,Conditioning (Psychology),Coronary Circulation,Coronary Disease,Electrocardiography,Electrophysiology,Heart,Hyperemia,Myocardial Reperfusion,Myocardial Reperfusion Injury,Swine,Time Factors,complications,etiology,methods,physiopathology},
number = {6 Pt 2},
