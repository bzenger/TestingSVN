journal = {European heart journal},
keywords = {201 thallium,cardiovascular stress test,diagnosis,dobutamine,dobutamine stress echocardiography,ecg abnormal,ecg lead,echocardiography,exercise stress test,inverted t wave,ischemia,left ventricular wall motion,myocardiai,myocardial ischemia,non-q wave myocardial infarction,perfusion,perfusion imaging,persistence,radionuclide imaging,radionuclide myocardial perfusion study,single photon emission computed tomography,spect,st segment depression,stress,t wave feature,t wave normalization,technetium tc 99m sestamibi},
