journal = {Clinica Chimica Acta},
keywords = {Animal models,Cardiac specificity,Cardiac troponin,Creatine kinase,Markers of myocardial damage},
number = {1-2},
pages = {65--74},
pmid = {11165200},
title = {{Cardiac troponins and creatine kinase content of striated muscle in common laboratory animals}},
volume = {304},
year = {2001}
