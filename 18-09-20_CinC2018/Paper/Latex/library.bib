publisher = {John Wiley {\&} Sons, Inc., New York, NY},
title = {{Predicting risk of cardiac events among ST-segment elevation myocardial infarction patients with conservatively managed non–infarct-related artery coronary artery disease: An analysis of the Duke Databank for Cardiovascular Disease}},
