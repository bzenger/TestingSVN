@article{Morimont2012,
abstract = {BACKGROUND: Peak first derivative of femoral artery pressure (arterial dP/dtmax) derived from fluid-filled catheter remains questionable to assess left ventricular (LV) contractility during shock. The aim of this study was to test if arterial dP/dtmax is reliable for assessing LV contractility during various hemodynamic conditions such as endotoxin-induced shock and catecholamine infusion., METHODS: Ventricular pressure-volume data obtained with a conductance catheter and invasive arterial pressure obtained with a fluid-filled catheter were continuously recorded in 6 anaesthetized and mechanically ventilated pigs. After a stabilization period, endotoxin was infused to induce shock. Catecholamines were transiently administrated during shock. Arterial dP/dtmax was compared to end-systolic elastance (Ees), the gold standard method for assessing LV contractility., RESULTS: Endotoxin-induced shock and catecholamine infusion lead to significant variations in LV contractility. Overall, significant correlation (r=0.51; p{\textless}0.001) but low agreement between the two methods were observed. However, a far better correlation with a good agreement were observed when positive-pressure ventilation induced an arterial pulse pressure variation (PPV){\textless}=11{\%} (r=0.77; p{\textless}0.001)., CONCLUSION: While arterial dP/dtmax and Ees were significantly correlated during various hemodynamic conditions, arterial dP/dtmax was more accurate for assessing LV contractility when adequate vascular filling, defined as PPV{\textless}=11{\%}, was achieved.},
author = {Morimont, Philippe and Lambermont, Bernard and Desaive, Thomas and Janssen, Nathalie and Chase, Geoffrey and D'Orio, Vincent},
doi = {10.1186/1471-2261-12-13},
file = {:Users/Brian/GoogleDrive/Papers/Morimont et al. - 2012 - Arterial dPdtmax accurately reflects left ventricular contractility during shock when adequate vascular filling.pdf:pdf},
isbn = {1471-2261},
issn = {1471-2261},
journal = {BMC Cardiovascular Disorders},
keywords = {aortic pressure,endotoxin-,left ventricular function,preload responsiveness,septic cardiomyopathy},
number = {1},
pages = {13},
pmid = {22380679},
publisher = {BioMed Central Ltd},
title = {{Arterial dP/dtmax accurately reflects left ventricular contractility during shock when adequate vascular filling is achieved}},
url = {http://bmccardiovascdisord.biomedcentral.com/articles/10.1186/1471-2261-12-13},
volume = {12},
year = {2012}
