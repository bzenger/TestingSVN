author = {McGann, Christopher J and Kholmovski, Eugene G and Oakes, Robert S and Blauer, Joshua J E and Daccarett, Marcos and Segerson, Nathan and Airey, Kelly J and Akoum, Nazem and Fish, Eric and Badger, Troy J and DiBella, Edward V R and Parker, Dennis and MacLeod, Rob S and Marrouche, Nassir F},
doi = {10.1016/j.jacc.2008.05.062},
