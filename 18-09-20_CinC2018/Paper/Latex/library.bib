title = {{Improved Left Ventricular Function After Chronic Left Ventricular Unloading}},
url = {http://www.sciencedirect.com/science/article/pii/S0003497596004377 http://www.sciencedirect.com/science/article/pii/S0003497596004377{\#} http://www.sciencedirect.com/science/article/pii/S0003497596004377/pdfft?md5=b5b2d79d599b9d90662f4f2e7ba3efcc{\&}pid=1-s2.0},
volume = {62},
