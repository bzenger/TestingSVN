title = {{Stress-induced QTc-interval shortening as an ancillary marker of ischemia in patients with complete left bundle branch block.}},
url = {http://content.wkhealth.com/linkback/openurl?sid=WKPTLP:landingpage{\&}an=01244665-200905000-00003},
volume = {10},
