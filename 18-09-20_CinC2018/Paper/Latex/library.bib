doi = {10.1016/j.jelectrocard.2017.09.013},
file = {:Users/Brian/GoogleDrive/Papers/Allencherril et al. - 2018 - Appropriateness of anteroseptal myocardial infarction nomenclature evaluated by late gadolinium enhancement.pdf:pdf},
issn = {15328430},
journal = {Journal of Electrocardiology},
keywords = {Anterior wall myocardial infarction,Anteroseptal myocardial infarction,Electrocardiography,Magnetic resonance imaging,Myocardial infarction,Q waves},
