isbn = {1525-0016 (Print)$\backslash$r1525-0016 (Linking)},
issn = {15250016},
journal = {Molecular Therapy},
keywords = {Adeno-associated vectors,Angiogenesis,Angiopoietin-1,Gene therapy,Muscle,Stem cells,Vascular endothelial growth factor},
number = {4},
pages = {450--459},
