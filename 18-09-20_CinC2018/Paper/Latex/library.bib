journal = {Circulation},
keywords = {Cardiovascular diseases,Epidemiology,Global health,Mortality},
number = {17},
pages = {1667--1678},
pmid = {26503749},
title = {{Global and regional patterns in cardiovascular mortality from 1990 to 2013}},
volume = {132},
year = {2015}
