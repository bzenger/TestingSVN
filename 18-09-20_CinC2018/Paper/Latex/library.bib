journal = {IEEE transactions on medical imaging},
keywords = {Anatomy, Cross-Sectional,Anatomy, Cross-Sectional: methods,Artificial Intelligence,Automatic Data Processing,Automatic Data Processing: methods,Finite Element Analysis,Heart,Heart: anatomy {\&} histology,Heart: physiopathology,Humans,Image Processing, Computer-Assisted,Image Processing, Computer-Assisted: methods,Information Storage and Retrieval,Information Storage and Retrieval: methods,Models, Cardiovascular,Pattern Recognition, Automated,Pattern Recognition, Automated: methods,Research Design,Research Design: statistics {\&} numerical data,Tomography, X-Ray Computed,Tomography, X-Ray Computed: methods},
