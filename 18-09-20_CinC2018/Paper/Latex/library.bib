file = {:Users/Brian/GoogleDrive/Papers/Bird et al. - 2003 - The human adult cardiomyocyte phenotype.pdf:pdf},
isbn = {0008-6363 (Print)},
issn = {00086363},
journal = {Cardiovascular Research},
keywords = {Adult human cardiomyocyte,Cadherin,Cell attachment,Cell isolation,Laminin,Myofibrillogenesis,Sarcomere},
number = {2},
