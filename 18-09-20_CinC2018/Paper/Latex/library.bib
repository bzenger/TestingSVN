title = {{Distribution and three-dimensional structure of intercellular junctions in canine myocardium.}},
url = {http://circres.ahajournals.org/content/64/3/563.abstract?ijkey=cc34698ee3704acd4c45117abf55939197591560{\&}keytype2=tf{\_}ipsecsha},
