pages = {165--168},
pmid = {14729788},
publisher = {BMJ Publishing Group},
title = {{ECGSIM: an interactive tool for studying the genesis of QRST waveforms}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/14729788 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC1768085 http://heart.bmj.com/content/90/2/165.short},
volume = {90},
