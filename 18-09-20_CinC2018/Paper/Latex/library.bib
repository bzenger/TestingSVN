journal = {Heart rhythm : the official journal of the Heart Rhythm Society},
keywords = {Aged,Body Surface Potential Mapping,Bundle-Branch Block,Bundle-Branch Block: etiology,Bundle-Branch Block: physiopathology,Cardiac Pacing, Artificial,Catheter Ablation,Confounding Factors (Epidemiology),Diagnosis, Differential,Electrocardiography,Electrophysiologic Techniques, Cardiac,Female,Follow-Up Studies,Heart Failure,Heart Failure: etiology,Heart Failure: physiopathology,Heart Ventricles,Heart Ventricles: physiopathology,Heart Ventricles: surgery,Humans,Male,Middle Aged,Myocardial Infarction,Myocardial Infarction: complications,Myocardial Infarction: epidemiology,Myocardial Infarction: physiopathology,Myocardial Infarction: surgery,Purkinje Fibers,Purkinje Fibers: physiopathology,Purkinje Fibers: surgery,Referenced,Tachycardia, Ventricular,Tachycardia, Ventricular: diagnosis,Tachycardia, Ventricular: epidemiology,Tachycardia, Ventricular: etiology,Tachycardia, Ventricular: physiopathology,Tachycardia, Ventricular: surgery},
