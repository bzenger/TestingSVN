issn = {00029149},
journal = {American Journal of Cardiology},
number = {10},
pages = {1356--1360},
pmid = {16275177},
title = {{Anatomic distribution of myocardial ischemia as a determinant of exercise-induced ST-segment depression}},
volume = {96},
year = {2005}
}
