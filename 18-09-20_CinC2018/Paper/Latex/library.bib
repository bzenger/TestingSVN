publisher = {Elsevier Masson SAS},
title = {{Effect of phenylephrine on focal atrial fibrillation originating in the pulmonary veins and superior vena cava}},
url = {http://dx.doi.org/10.1016/S0735-1097(00)00792-0 http://linkinghub.elsevier.com/retrieve/pii/S0735109700007920},
volume = {36},
