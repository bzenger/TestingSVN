keywords = {Animals,Coronary Disease,Electrocardiography,Electrophysiology,Heart Conduction System,Pericardium,Phenol,Phenols,Swine,Sympathectomy, Chemical,Sympathetic Nervous System,anatomy {\&} histology,pathology,physiopathology},
