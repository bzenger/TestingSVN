pages = {155--158},
publisher = {Excerpta Medica},
title = {{Early diagnosis of acute myocardial infarction in patients without ST-segment elevation}},
url = {https://www.sciencedirect.com/science/article/pii/S0002914998008169},
volume = {83},
year = {1999}
