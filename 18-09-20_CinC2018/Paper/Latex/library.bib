file = {:Users/Brian/GoogleDrive/Papers/Mortazavi et al. - 2008 - Mapping and quantifying mammalian transcriptomes by RNA-Seq.pdf:pdf},
isbn = {1548-7105 (Electronic)$\backslash$r1548-7091 (Linking)},
issn = {1548-7091},
journal = {Nature methods},
number = {7},
pages = {621--628},
pmid = {18516045},
title = {{Mapping and quantifying mammalian transcriptomes by RNA-Seq.}},
volume = {5},
year = {2008}
}
